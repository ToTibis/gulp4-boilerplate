{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "entry",
        "corejs": "3.8",
        "modules" : "commonjs",
        "debug": false
      }
    ]
  ],
  "plugins": [
    ["module-resolver", {
      "alias": {
        "modules": "./node_modules"
      }
    }],
    ["@babel/plugin-transform-runtime",
      {
        "regenerator": true
      }
    ]
  ]
}